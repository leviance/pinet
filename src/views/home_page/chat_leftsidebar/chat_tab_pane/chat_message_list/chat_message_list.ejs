<div class="px-2">
  <h5 class="mb-3 px-3 font-size-16">Recent</h5>

  <div class="chat-message-list" data-simplebar>

      <ul id="chat-message-list" class="list-unstyled chat-list chat-user-list">
          <% list_messages.forEach(function(message){ %>
            <% 
            let message_class = "";
            let dot_status = "offline";
            if(message.type == "chat_group") {
                message_class = "show-modal-chat-group";
                dot_status = "online";
            }
            if(message.type == "chat_personal") message_class = "show-modal-chat-persional"
            %>

            <li data-uid="<%= message.partner_id %>" class="<%= message_class %> typing" chat-type="<%= message.type %>" >
                <a href="javascript: void(0);">
                    <div class="media">
  
                        <div class="chat-user-img <%= dot_status %> align-self-center mr-3">
                            <img src="/assets/images/users/<%= message.partner_avatar %>" class="rounded-circle avatar-xs" alt="">
                            <span class="user-status"></span>
                        </div>
  
                        <div class="media-body overflow-hidden">
                            <h5 class="text-truncate font-size-15 mb-1"><%= message.partner_username %></h5>
                            <p class="chat-user-message text-truncate mb-0"><%= message.message %></p>
                        </div>
                        <div class="font-size-11"><%= message.human_time %></div>
                    </div>
                </a>
            </li>
          <% }) %>
        

